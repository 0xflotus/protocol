<{{tag}} {{#if id}}id="{{id}}"{{/if}} class="mds-pattern-title">
  {{#if id}}
    <a href="#{{id}}" class="mds-pattern-anchor">{{> @partial-block }}</a>
  {{else}}
    {{> @partial-block }}
  {{/if}}
</{{tag}}>
{{#if labels}}
  <ul class="mds-pattern-labels">
    {{#each labels as |label|}}
      <li class="mds-label mds-label--{{label}}">
        {{label}}
      </li>
    {{/each}}
  </ul>
{{/if}}
